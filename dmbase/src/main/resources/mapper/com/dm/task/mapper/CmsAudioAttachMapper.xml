<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dm.task.sqldao.CmsAudioAttachMapper">
	<resultMap id="BaseResultMap" type="com.dm.task.model.CmsAudioAttach">
		<result column="audio_id" property="audioId" jdbcType="INTEGER" />
		<result column="attachment_id" property="attachmentId"
			jdbcType="INTEGER" />
	</resultMap>
	<sql id="Base_Column_List">
		audio_id,attachment_id
	</sql>
	<insert id="insert" parameterType="com.dm.task.model.CmsAudioAttach">
		insert into cms_audio_attach
		(audio_id, attachment_id)
		values (#{audioId,jdbcType=INTEGER},
		#{attachmentId,jdbcType=INTEGER})
	</insert>
	<insert id="insertSelective" parameterType="com.dm.task.model.CmsAudioAttach">
		insert into cms_audio_attach
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="audioId != null">
				audio_id,
			</if>
			<if test="attachmentId != null">
				attachment_id,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="audioId != null">
				#{audioId,jdbcType=INTEGER},
			</if>
			<if test="attachmentId != null">
				#{attachmentId,jdbcType=INTEGER},
			</if>
		</trim>
	</insert>
	<select id="selectAttaList" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from cms_audio_attach
		where audio_id = #{id}
	</select>
	<delete id="deleteAttaByContentId">
		delete from cms_audio_attach where audio_id = #{id}
	</delete>
</mapper>