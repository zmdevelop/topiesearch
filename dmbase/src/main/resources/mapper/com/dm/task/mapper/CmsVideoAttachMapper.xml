<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dm.task.sqldao.CmsVideoAttachMapper">
	<resultMap id="BaseResultMap" type="com.dm.task.model.CmsVideoAttach">
		<result column="attachment_id" property="attachmentId"
			jdbcType="INTEGER" />
		<result column="video_id" property="videoId" jdbcType="INTEGER" />
	</resultMap>
	<sql id="Base_Column_List">
		video_id,attachment_id
	</sql>
	<insert id="insert" parameterType="com.dm.task.model.CmsVideoAttach">
		insert into cms_video_attach (attachment_id, video_id)
		values (#{attachmentId,jdbcType=INTEGER}, #{videoId,jdbcType=INTEGER})
	</insert>
	<insert id="insertSelective" parameterType="com.dm.task.model.CmsVideoAttach">
		insert into cms_video_attach
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="attachmentId != null">
				attachment_id,
			</if>
			<if test="videoId != null">
				video_id,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="attachmentId != null">
				#{attachmentId,jdbcType=INTEGER},
			</if>
			<if test="videoId != null">
				#{videoId,jdbcType=INTEGER},
			</if>
		</trim>
	</insert>
	<select id="selectAttaList" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from cms_video_attach
		where video_id = #{id}
	</select>
	<delete id="deleteAttaByContentId">
		delete from cms_video_attach where video_id = #{id}
	</delete>
</mapper>